#include <iostream>
#include <climits>
using namespace std;

int main() {
    int arr[] = {10, 20, 4, 45, 99};  // Hardcoded input
    int n = sizeof(arr)/sizeof(arr[0]);
    int f = INT_MIN, s = INT_MIN, t = INT_MIN;

    for (int i = 0; i < n; i++) {
        if (arr[i] > f) { t = s; s = f; f = arr[i]; }
        else if (arr[i] > s && arr[i] < f) { t = s; s = arr[i]; }
        else if (arr[i] > t && arr[i] < s) t = arr[i];
    }

    if (s != INT_MIN) cout << "Second: " << s << endl;
    else cout << "Second largest does not exist\n";

    if (t != INT_MIN) cout << "Third: " << t << endl;
    else cout << "Third largest does not exist\n";
}
